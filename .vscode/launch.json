{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug with GDB",
      "type": "cppdbg",
      "request": "launch",
      //"program": "${workspaceFolder}/abo_predict", // Path to your executable
      "program": "${workspaceFolder}/bin/dd_test",
      //"program": "${workspaceFolder}/bin/elect_test",
      //"program": "${workspaceFolder}/bin/gridsearch_test",
      //"program": "${workspaceFolder}/bin/gridsearch_test_best",
      //"program": "${workspaceFolder}/bin/gridsearch_eurusd_test_best",
      //"program": "${workspaceFolder}/bin/EURUSD_test",
      //"program": "${workspaceFolder}/bin/baseline_qrd_rls_eurusd",
      //"program": "${workspaceFolder}/bin/baseline_qrd_rls_elect",
      //"program": "${workspaceFolder}/bin/baseline_k_rls_elect",
      //"program": "${workspaceFolder}/bin/baseline_k_rls_eurusd",
      //"program": "${workspaceFolder}/bin/real_cond_num_test",
      //"program": "${workspaceFolder}/bin/timing_test",
      "args": [
        "--n_trials 300",
        "--lags 20",
        "--kfolds 8",
        "--model_name krls"
      ], // Command-line arguments
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build", // Run the build task before debugging
      "miDebuggerPath": "/usr/bin/gdb", // Path to GDB
      "logging": {
        "trace": true,
        "traceResponse": true,
        "engineLogging": true
      }
    }
  ]
}
